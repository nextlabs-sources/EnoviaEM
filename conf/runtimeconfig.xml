    <policy-controller>
        <config-file>openaz-pep-on-prem.properties</config-file>
        <appname>Engineering Central</appname>
        <default-action>allow</default-action>
        <default-message>PDP connection timeout</default-message>
        <default-na-action>deny</default-na-action>
        <default-na-message>Not matching allow policy</default-na-message>
        <default-indeterminate-action>deny</default-indeterminate-action>
        <default-indeterminate-message>PDP error, please contact Administrator for help.</default-indeterminate-message>
        <connect-retry-count>5</connect-retry-count>
        <connect-retry-timer>100000</connect-retry-timer>
    </policy-controller>
    <system>
        <config-reload-interval>36000</config-reload-interval>
        <!-- notification: either Notice or FrameworkException -->
        <notification-method>Notice</notification-method>
        <replace-user-agent>false</replace-user-agent>
        <username-to-lower>false</username-to-lower>
    </system>
</runtime-configuration>
